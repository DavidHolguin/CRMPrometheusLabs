{
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920],
    "domains": ["unsplash.com", "images.unsplash.com"],
    "formats": ["image/webp", "image/avif"],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false
  }
}